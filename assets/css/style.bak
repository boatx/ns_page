}

.left3
{
	background: #f9fcf7; /* old browsers */
	background: -moz-linear-gradient(top, #f9fcf7 0%, #EEF9EA 45%, #DAE8EF 100%); /* firefox */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f9fcf7), color-stop(45%,#EEF9EA), color-stop(100%,#DAE8EF)); /* webkit */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9fcf7', endColorstr='#DAE8EF',GradientType=0 ); /* ie */
	background: -o-linear-gradient(top, #f9fcf7 0%,#EEF9EA 45%,#DAE8EF 100%); /* opera */
    position:relative;
    background-color:red;
    width:19.80198%;
