body
{
    height:100%;
    margin: 0;
    padding: 0;
    background-color:#3ABC33;
    background: rgb(249,252,242); /* Old browsers */
    background: -moz-linear-gradient(top, rgba(249,252,242,1) 0%, rgba(249,252,242,1) 0%, rgba(149,209,135,1) 81%, rgba(56,188,49,1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(249,252,242,1)), color-stop(0%,rgba(249,252,242,1)), color-stop(81%,rgba(149,209,135,1)), color-stop(100%,rgba(56,188,49,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, rgba(249,252,242,1) 0%,rgba(249,252,242,1) 0%,rgba(149,209,135,1) 81%,rgba(56,188,49,1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, rgba(249,252,242,1) 0%,rgba(249,252,242,1) 0%,rgba(149,209,135,1) 81%,rgba(56,188,49,1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, rgba(249,252,242,1) 0%,rgba(249,252,242,1) 0%,rgba(149,209,135,1) 81%,rgba(56,188,49,1) 100%); /* IE10+ */
    background: linear-gradient(to bottom, rgba(249,252,242,1) 0%,rgba(249,252,242,1) 0%,rgba(149,209,135,1) 81%,rgba(56,188,49,1) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9fcf2', endColorstr='#38bc31',GradientType=0 ); /* IE6-9 */
    background-repeat: no-repeat;
    background-attachment: fixed;
}

#headCont
{
    width:100%;
    height:115px;
    position:fixed;
    top:0;
    z-index:100;
}

header
{
    background: rgb(59,153,204); /* Old browsers */
    background: -moz-linear-gradient(top, rgba(59,153,204,1) 0%, rgba(255,255,255,1) 54%, rgba(202,239,179,1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(59,153,204,1)), color-stop(54%,rgba(255,255,255,1)), color-stop(100%,rgba(202,239,179,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, rgba(59,153,204,1) 0%,rgba(255,255,255,1) 54%,rgba(202,239,179,1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, rgba(59,153,204,1) 0%,rgba(255,255,255,1) 54%,rgba(202,239,179,1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, rgba(59,153,204,1) 0%,rgba(255,255,255,1) 54%,rgba(202,239,179,1) 100%); /* IE10+ */
    background: linear-gradient(to bottom, rgba(59,153,204,1) 0%,rgba(255,255,255,1) 54%,rgba(202,239,179,1) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3b99cc', endColorstr='#caefb3',GradientType=0 ); /* IE6-9 */
	margin-bottom:0px;
	margin-top:0px;
	height : 75px;
	text-align:center;
}


nav
{
	background: #f9fcf7; /* old browsers */
	background: -moz-linear-gradient(top, #f9fcf7 0%, #EEF9EA 45%, #DAE8EF 100%); /* firefox */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f9fcf7), color-stop(45%,#EEF9EA), color-stop(100%,#DAE8EF)); /* webkit */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9fcf7', endColorstr='#DAE8EF',GradientType=0 ); /* ie */
	background: -o-linear-gradient(top, #f9fcf7 0%,#EEF9EA 45%,#DAE8EF 100%); /* opera */

	-moz-box-shadow: 3px 3px 3px #666666;
	-webkit-box-shadow: 3px 3px 3px #666666;
	box-shadow: 0px 3px 6px #666666;

	height:40px;
	/*-moz-border-radius:20px;
	-webkit-border-radius:20px;
	border-radius:20px;*/
}


nav a
{
 font-family:Serif;
 display:block;
 text-decoration:none;
 color:#2C5E34;
 font-weight:bold;
 font-size:15px;
 height:100%;
}

nav a:hover
{
 font-family:Serif;
 text-decoration:none;
 color:white;
 background-color:#2C5E34;
 font-weight:bold;
 font-size:15px;
}

nav ul
{
	list-style-type:none;
	margin:0;
	text-align:center;
	width:100%;
	float:left;
    display:block;
}

nav li
{
	position:relative;
	float:left;
    line-height:300%;
    width:120px;
}

nav ul ul
{
  position:absolute;
  z-index:500;
}

nav ul ul,
nav ul li:hover ul
{
  display:none;
}

nav ul li:hover ul
{
    background: #f9fcf7; /* old browsers */
    background: -moz-linear-gradient(top, #f9fcf7 0%, #EEF9EA 45%, #DAE8EF 100%); /* firefox */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f9fcf7), color-stop(45%,#EEF9EA), color-stop(100%,#DAE8EF)); /* webkit */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9fcf7', endColorstr='#DAE8EF',GradientType=0 ); /* ie */
    background: -o-linear-gradient(top, #f9fcf7 0%,#EEF9EA 45%,#DAE8EF 100%); /* opera */

    -moz-box-shadow: 3px 3px 3px #666666;
    -webkit-box-shadow: 3px 3px 3px #666666;
    box-shadow: 0px 3px 6px #666666;
    display:block;
    padding:0;
}

#main
{
  min-height:100%;
  position:relative;
  margin-left: auto;
  margin-right: auto;
  margin-top:125px;
  width:75%; /*1010px*/
}

#content
{
  width:100%;
  height:80%;
  display:table;
}

.oneBig
{
    float:left;
    position:relative;
    left:10.148515%;
    right:10.148515%;
    width:79.70297%;
    height:200px;
    margin-top:35px;
    margin-bottom:35px;
}

#slider div
{
    bottom:0;
    right:0;
    position:absolute;
    width:100%;
    height:100%;
    margin:0;
    display:none;
    z-index:0;
}

#slider div img
{
    width:100%;
    height:100%;
    margin:0;
}

#slider #control
{
    bottom:0;
    right:0;
    margin-bottom:5%;
    margin-right:10%;
    width:auto;
    height:auto;
    display:block;
    z-index:2;
}


.left1
{
	background: #f9fcf7; /* old browsers */
	background: -moz-linear-gradient(top, #f9fcf7 0%, #EEF9EA 45%, #DAE8EF 100%); /* firefox */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f9fcf7), color-stop(45%,#EEF9EA), color-stop(100%,#DAE8EF)); /* webkit */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9fcf7', endColorstr='#DAE8EF',GradientType=0 ); /* ie */
	background: -o-linear-gradient(top, #f9fcf7 0%,#EEF9EA 45%,#DAE8EF 100%); /* opera */
    position:relative;
    width:19.80198%; /*200px/1010px*/
    height:200px;
    left:10.148515%;
    float:left;
    clear:left;
}

