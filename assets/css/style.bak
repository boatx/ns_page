    left:30.445545%;
    float:left;
}

footer{
    background-color: #3ABC33; /* Old browsers */
	width:100%;
	height:5%;
	margin:10px 0px 0px 0px ;
	padding:2px;
	text-align:center;
	color:#ffffff;
	font-size:0.9em;
	font-family: Tahoma, Arial, Helvetica, Sans-serif;
    clear:left;
}


section
{
	width:85%;
	padding:15px;
	text-align:justify;
	display:table-cell;
}

article
{
  font-weight:normal;
  font-size:1.0em;
  text-align:justify;
  font-family: Tahoma, Arial, Helvetica, Sans-serif;
  text-indent:2em;
  color:black;
  margin:0;
  padding:0;
}

